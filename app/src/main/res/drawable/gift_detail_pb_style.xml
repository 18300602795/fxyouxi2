<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="10dp" />
            <stroke android:color="@color/gray1" android:width="0.8dp"></stroke>
            <gradient
                android:angle="0"
                android:endColor="@color/white"
                android:startColor="@color/white" />
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="10dp" />
                <gradient
                    android:angle="90.0"
                    android:centerColor="#ac6079"
                    android:centerY="0.75"
                    android:endColor="#6c213a"
                    android:startColor="#e71a5e" />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="10dp" />
                <stroke android:color="@color/gray1" android:width="0.8dp"></stroke>

                <gradient
                    android:angle="0"
                    android:centerColor="@color/bg_selected"
                    android:endColor="@color/bg_selected"
                    android:startColor="@color/bg_selected" />
            </shape>
        </clip>
    </item>
</layer-list>
