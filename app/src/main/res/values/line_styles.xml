        <item name="android:background">@color/aile_line_gray</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">0.5dp</item>
    </style>

    <style name="line_color_drak">
        <item name="android:background">@color/bg_gray_ccc</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">0.5dp</item>
    </style>

    <style name="line_color_drak_v">
        <item name="android:background">@color/bg_gray_ccc</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_width">0.5dp</item>
    </style>


    <style name="line_color_v">
        <item name="android:background">@color/bg_gray</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_width">0.5dp</item>
    </style>
    <style name="line_split_color">
        <item name="android:background">@color/bg_comm</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
